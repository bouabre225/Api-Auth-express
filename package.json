{
  "name": "api-rest-auth",
  "version": "1.0.0",
  "description": "Implémenter une API REST d'authentification complète intégrant plusieurs méthodes d'authentification ( NodeJS + Express)",
  "type": "module",
  "imports": {
    "#lib/*": "./src/lib/*.js",
    "#controllers/*": "./src/controllers/*.js",
    "#services/*": "./src/services/*.js",
    "#middlewares/*": "./src/middlewares/*.js",
    "#routes/*": "./src/routes/*.js",
    "#schemas/*": "./src/schemas/*.js",
    "#dto/*": "./src/dto/*.js"
  },
  "scripts": {
    "dev": "node --watch src/index.js",
    "start": "node src/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "argon2"
    ]
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "jest": "^30.2.0",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "nodemailer": "^7.0.12",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^30.2.0",
    "prisma": "^7.2.0",
    "supertest": "^7.1.4",
    "vitest": "^4.0.16"
  },
  "main": "prisma.config.js",
  "directories": {
    "test": "tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
